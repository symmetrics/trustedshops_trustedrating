<config>
    <acl>
        <resources>
            <admin>
                <children>
                    <system>
                        <children>
                            <config>
                                <children>
                                    <trustedrating translate="title" module="trustedrating">
                                        <title>Trusted Shops</title>
                                        <sort_order>50</sort_order>
                                    </trustedrating>
                                </children>
                            </config>
                        </children>
                    </system>
                </children>
            </admin>
        </resources>
    </acl>
</config>
